for STR in STRS:
    while i < len(dna):
        if STR == dna[i:i + len(STR)]:
            a += 1
            i += len(STR)
            if a > max_a:
                max_a = a
        elif STR == dna[i:i + len(STR)]:
            b += 1
            i += len(STR)
            if b > max_b:
                max_b = b
        elif STR == dna[i:i + len(STR)]:
            c += 1
            i += len(STR)
            if c > max_c:
                max_c = c
        else:
            i += 1




while i < len(dna):
    if STR[0] == dna[i:i + len(STR[0])]:
        a += 1
        i += len(STR[0])
        if a > max_a:
            max_a = a
    elif STR[1] == dna[i:i + len(STR[1])]:
        b += 1
